
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Gestor {
  id Int @id @default(autoincrement())
  nome String?
  email String @unique
  senha String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}